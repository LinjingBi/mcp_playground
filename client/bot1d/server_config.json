{
    "mcpServers": {
      "fetch": {
        "command": "uvx",
        "args": [
          "mcp-server-fetch"
        ]
      },
      "self-filesystem": {
        "command": "<which uv>",
        "args" : [
             "--directory",
             "<path_to_to_local.py>",
             "run",
             "to_local.py"
          ]
      }
    }
  }